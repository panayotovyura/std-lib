{
    "name": "php-solution/std-lib",
    "type": "library",
    "description": "This library contains frequently used php features",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Kalinin",
            "email": "kalinick@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0"
    },
    "suggest": {
    },
    "autoload":     {
        "psr-4": { "PhpSolution\\StdLib\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}